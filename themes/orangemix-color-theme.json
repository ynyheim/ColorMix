{
  "name": "OrangeMix (Dark)",
  "type": "dark",
  "colors": {
    "editor.background": "#191919",
    "editor.foreground": "#CFCFCF",
    "editor.lineHighlightBackground": "#222222",
    "editorIndentGuide.background": "#333333",
    "editor.findMatchHighlightBackground": "#7F260D",
    "editor.findMatchBackground": "#7F260D",
    "statusBar.background": "#7F4619",
    "statusBar.foreground": "#D1D1D1",
    "editorCursor.foreground": "#F38630",
    "tab.activeForeground" : "#FFF",
    "tab.activeBorderTop": "#DB792B"
  },
  "tokenColors": [
    //
    // SOURCE CODE
    //
    {
      "name": "Source",
      "scope": [
        "entity",
        "source",
        "meta.namespace-block.cpp",
        "variable"
      ],
      "settings": {
        "foreground": "#d1d1d1"
      }
    },
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "punctuation.end.definition.comment",
        "punctuation.start.definition.comment",
        "string.quoted.docstring.multi.python"
      ],
      "settings": {
        "foreground": "#8EAB6F"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.control.directive.include",
        "keyword.operator.expression",
        "keyword.operator.new",
        "keyword.operator.asp",
        "punctuation.definition.typeparameters.begin",
        "punctuation.definition.typeparameters.end",
        "punctuation.parenthesis.open",
        "punctuation.parenthesis.close",
        "storage.modifier",
        "storage.type.asp",
        "storage.type.js",
        "storage.type.ts",
        "storage.type.function",
        "storage.type.interface",
        "storage.type.enum",
        "storage.type.c",
        "storage.type.class",
        "storage.type.cpp",
        "storage.type.modifier",
        "storage.type.namespace",
        "storage.type.def",
        "storage.type.property",
        "storage.type.type",
        "support.class.builtin",
        "support.function",
        "support.type.primitive.ts",
        "support.constant.property-value.css",
        "variable.language.this",
        "comment.block.ts storage.type.internaldeclaration.ts"
      ],
      "settings": {
        "foreground": "#9E9E9E",
        "fontStyle": ""
      }
    },
    {
      "name": "Keyword Warning",
      "scope": [
        "meta.preprocessor.diagnostic",
        "keyword.other.important.css",
        "meta.tag.metadata.script"
      ],
      "settings": {
        "foreground": "#D54016"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation",
        "punctuation.separator.namespace.access.cpp"
      ],
      "settings": {
        "foreground": "#9E9E9E"
      }
    },
    {
      "name": "Operator",
      "scope": [
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#6e6e6e",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Important Keyword or Operator",
      "scope": [
        "keyword.operator.optional",
        "storage.type.function.arrow.ts",
        "keyword.operator.arrow.cs",
        "keyword.control.case.cs",
        "keyword.operator.cast.cpp",
        "keyword.other.lock.cs",
        "punctuation.separator.pointer-access.c",
        "keyword.other.debugger"
      ],
      "settings": {
        "foreground": "#DB792B",
        "fontStyle": ""
      }
    },
    {
      "name": "Definition",
      "scope": [
        "entity.name.function.target.makefile",
        "entity.name.type.class",
        "entity.name.type.struct",
        "entity.name.type.interface",
        "entity.name.type.enum",
        "entity.name.type.delegate",
        "storage.type.template",
        "entity.name.type.alias"
      ],
      "settings": {
        "foreground": "#DB792B",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Generic Type Definition",
      "scope": [
        "entity.name.type.type-parameter",
        "meta.type.parameters",
        "meta.type.parameters.ts"
      ],
      "settings": {
        "foreground": "#039be5"
      }
    },
    {
      "name": "Type",
      "scope": [
        "storage.type",
        "support.class",
        "entity.other.inherited-class",
        "entity.name.type.inherited.cpp",
        "meta.class-struct-block.cpp entity.name.type.cpp",
        "meta.parameters.ts entity.name.type.ts",
        "meta.field.declaration.ts entity.name.type.ts",
        "meta.return.type.ts entity.name.type.ts",
        "new.expr.ts entity.name.type.ts",
        "meta.type.annotation.ts entity.name.type.ts"
      ],
      "settings": {
        "foreground": "#D66E1C"
      }
    },
    {
      "name": "Class Member Definition",
      "scope": [
        "entity.name.variable.property.cs",
        "meta.class.ts meta.method.declaration.ts meta.definition.method.ts",
        "meta.class.ts meta.function.ts meta.definition.function.ts",
        "meta.class.ts meta.field.declaration.ts meta.definition.property.ts",
        "meta.function.ts meta.definition.function.ts"
      ],
      "settings": {
        "foreground": "#C77C4F"
      }
    },
    {
      "name": "Conditional Case",
      "scope": [
        "case-clause.expr.ts variable.other.constant.property.ts",
        "case-clause.expr.ts variable.other.property.ts"
      ],
      "settings": {
        "foreground": "#BA951C"
      }
    },
    {
      "name": "Constant",
      "scope": [
        "constant",
        "support.type.primitive"
      ],
      "settings": {
        "foreground": "#FFA000"
      }
    },
    {
      "name": "Constant Boolean",
      "scope": [
        "constant.language.boolean"
      ],
      "settings": {
        "foreground": "#FFA000"
      }
    },
    {
      "name": "Constant Number",
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#ffca28"
      }
    },
    {
      "name": "Constant Null",
      "scope": [
        "constant.language.null",
        "constant.language.undefined",
        "keyword.operator.null-coalescing.cs",
        "keyword.operator.null-conditional.cs"
      ],
      "settings": {
        "foreground": "#E24417",
        "fontStyle": ""
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "punctuation.definition.string.begin.cs",
        "punctuation.definition.string.end.cs"
      ],
      "settings": {
        "foreground": "#BA951C"
      }
    },
    {
      "name": "String Escape",
      "scope": [
        "punctuation.definition.interpolation.begin",
        "punctuation.definition.interpolation.end",
        "constant.character.escape",
        "string.template.ts punctuation.definition.template-expression"
      ],
      "settings": {
        "foreground": "#DB792B"
      }
    },
    {
      "name": "Regexp",
      "scope": [
        "string.regexp"
      ],
      "settings": {
        "foreground": "#ffca28"
      }
    },
    {
      "name": "Regexp Group",
      "scope": [
        "punctuation.definition.group.regexp"
      ],
      "settings": {
        "foreground": "#039be5",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Regexp Operator",
      "scope": [
        "keyword.operator.quantifier.regexp",
        "constant.character.escape.backslash.regexp",
        "keyword.operator.negation.regexp"
      ],
      "settings": {
        "foreground": "#26c6da",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Property",
      "scope": [
        "support.type.property-name",
        "variable.other.object.property",
        "entity.name.variable.property",
        "entity.name.tag.yaml"
      ],
      "settings": {
        "foreground": "#d1d1d1"
      }
    },
    {
      "name": "Preprocessor Directive",
      "scope": [
        "meta.preprocessor",
        "keyword.preprocessor",
        "punctuation.separator.hash",
        "keyword.control.directive."
      ],
      "settings": {
        "foreground": "#039be5"
      }
    },
    {
      "name": "Preprocessor Symbol",
      "scope": [
        "entity.name.variable.preprocessor.symbol",
        "string.unquoted.preprocessor.message",
        "entity.other.attribute-name.pragma.preprocessor",
        "entity.name.function.preprocessor"
      ],
      "settings": {
        "foreground": "#26c6da"
      }
    },
    {
      "name": "Include Symbol",
      "scope": [
        "string.quoted.double.include",
        "string.quoted.other.lt-gt.include"
      ],
      "settings": {
        "foreground": "#d0d0e0"
      }
    },
    {
      "name": "Error",
      "scope": [
        "keyword.control.directive.diagnostic.error"
      ],
      "settings": {
        "foreground": "#c62828"
      }
    },
    {
      "name": "CSS Color",
      "scope": [
        "constant.other.color.rgb-value.hex.css",
        "punctuation.definition.constant.css"
      ],
      "settings": {
        "foreground": "#ffa726"
      }
    },
    {
      "name": "CSS Selector",
      "scope": [
        "entity.other.attribute-name.id.css",
        "entity.name.tag.css",
        "entity.other.attribute-name.class.css"
      ],
      "settings": {
        "foreground": "#D66E1C"
      }
    },
    {
      "name": "CSS Property",
      "scope": [
        "support.type.property-name.css"
      ],
      "settings": {
        "foreground": "#e0e0e0"
      }
    },
    {
      "name": "C# Comment Tag",
      "scope": [
        "punctuation.definition.tag.cs",
        "entity.name.tag.localname.cs",
        "entity.other.attribute-name.localname.cs"
      ],
      "settings": {
        "foreground": "#5B7148"
      }
    },
    //
    // CONFIGURATION / DATA
    //
    {
      "name": "JSON Constant",
      "scope": [
        "constant.language.json"
      ],
      "settings": {
        "foreground": "#D66E1C"
      }
    },
    {
      "name": "JSON Property",
      "scope": [
        "support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#c0c0c0"
      }
    },
    {
      "name": "JSON Content",
      "scope": [
        "string.quoted.double.json.comments"
      ],
      "settings": {
        "foreground": "#BA951C"
      }
    },
    {
      "name": "SGML Definitions",
      "scope": [
        "keyword.other.doctype.xml"
      ],
      "settings": {
        "foreground": "#F38630",
        "fontStyle": "underline"
      }
    },
    {
      "name": "SGML Punctation",
      "scope": [
        "entity.name.tag",
        "meta.tag",
        "punctuation.definition.tag",
        "punctuation.definition.string.begin.xml",
        "punctuation.definition.string.begin.html",
        "punctuation.definition.string.end.xml",
        "punctuation.definition.string.end.html"
      ],
      "settings": {
        "foreground": "#9E9E9E"
      }
    },
    {
      "name": "SGML Attribute Name",
      "scope": [
        "entity.other.attribute-name",
        "entity.other.attribute-name.localname"
      ],
      "settings": {
        "foreground": "#C77C4F"
      }
    },
    {
      "name": "SGML Attribute Value",
      "scope": [
        "string.quoted.double.xml",
        "string.quoted.double.html",
        "meta.jsx.children"
      ],
      "settings": {
        "foreground": "#C6C6C6"
      }
    },
    {
      "name": "SGML Preprocessor Attribute Value",
      "scope": [
        "meta.tag.preprocessor.xml string.quoted.double.xml",
        "meta.tag.preprocessor.xml string.quoted.double.html"
      ],
      "settings": {
        "foreground": "#26c6da"
      }
    },
    {
      "name": "SGML Namespace Identifier",
      "scope": [
        "entity.name.tag.namespace",
        "punctuation.separator.namespace",
        "entity.other.attribute-name.namespace"
      ],
      "settings": {
        "foreground": "#D66E1C"
      }
    },
    {
      "name": "SGML Content",
      "scope": [
        "text.xml",
        "text.html"
      ],
      "settings": {
        "foreground": "#BDA95B"
      }
    },
    {
      "name": "SGML Constant",
      "scope": [
        "punctuation.definition.constant",
        "constant.character.entity"
      ],
      "settings": {
        "foreground": "#f57c00"
      }
    },
    {
      "name": "INI Section Name",
      "scope": [
        "entity.name.section.group-title.ini"
      ],
      "settings": {
        "foreground": "#D66E1C"
      }
    },
    //
    // TEXT
    //
    {
      "name": "Text",
      "scope": [
        "text",
        "text.html.markdown"
      ],
      "settings": {
        "foreground": "#C6C6C6"
      }
    },
    {
      "name": "Text Heading",
      "scope": [
        "entity.name.section.markdown"
      ],
      "settings": {
        "foreground": "#D1D1D1",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Text Heading Tag",
      "scope": [
        "punctuation.definition.heading.markdown"
      ],
      "settings": {
        "foreground": "#9E9E9E"
      }
    },
    {
      "name": "Text List",
      "scope": [
        "punctuation.definition.list.begin.markdown"
      ],
      "settings": {
        "foreground": "#BDA95B"
      }
    },
    {
      "name": "Text Quote",
      "scope": [
        "markup.quote.markdown"
      ],
      "settings": {
        "foreground": "#C96F28"
      }
    },
    {
      "name": "Text Inline Code",
      "scope": [
        "markup.inline.raw.string.markdown",
        "text.aspnetcorerazor"
      ],
      "settings": {
        "foreground": "#BDA95B"
      }
    },
    {
      "name": "Text Code Block Punctation",
      "scope": [
        "punctuation.definition.markdown"
      ],
      "settings": {
        "foreground": "#039be5",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Text Code Block Category",
      "scope": [
        "fenced_code.block.language"
      ],
      "settings": {
        "foreground": "#26c6da"
      }
    },
    {
      "name": "Text Link Title",
      "scope": [
        "string.other.link.title.markdown",
        "punctuation.definition.string.begin.markdown",
        "punctuation.definition.string.end.markdown"
      ],
      "settings": {
        "foreground": "#D66E1C"
      }
    },
    {
      "name": "Text Link",
      "scope": [
        "markup.underline.link.markdown",
        "punctuation.definition.metadata.markdown"
      ],
      "settings": {
        "foreground": "#9E9E9E"
      }
    }
  ]
}